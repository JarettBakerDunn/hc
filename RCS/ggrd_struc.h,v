@struct ggrd_vel{
  
  CPREC *vr,*vt,*vp;	/* velocity field */
  int n[5];		/* dimensions in r, theta, and 
				   phi directions */
  int ntnp,nrntnp;		/*  */
  CPREC *rlevels;		/* levels where velocities are 
				   specified */
  CPREC dtheta,dphi;		/* spacing in theta and phi */
  CPREC *vtimes;		/* times at which velocities 
				   are specified */
  CPREC velscale,rcmb;
  int nvtimes,nvtimes3;			/* number of times where specified 
				 */
  hc_boolean init,		/* initialized? */
    history,
    read_gmt;		/*  read GMT grd files or binary format?*/
  int amode;
};
